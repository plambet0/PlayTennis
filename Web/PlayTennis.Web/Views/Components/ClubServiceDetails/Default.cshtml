@model ClubDetailsViewModel;
@{ ViewData["Title"] = "Default"; }

<div class="form-group row">
    <label class="col-sm-2 col-form-label">Salon</label>
    <div class="col-sm-10">
        <div class="form-control">@Model.Name</div>
    </div>
</div>
<div class="form-group row">
    <label class="col-sm-2 col-form-label">Address</label>
    <div class="col-sm-10">
        <div class="form-control">@Model.Town, @Model.Address</div>
    </div>
</div>

